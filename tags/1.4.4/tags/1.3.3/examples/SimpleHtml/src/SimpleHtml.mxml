<mx:Application 
	xmlns:mx="http://www.adobe.com/2006/mxml" 
    xmlns:flexiframe="http://code.google.com/p/flex-iframe/"
	layout="absolute"
	creationComplete="myFrame.visible=true">

    <mx:Panel title="Simple HTML Test application v1.0"
    		width="100%"
    		height="100%">
        
    	<flexiframe:IFrame
    		id="myFrame"
    		width="100%"
    		height="100%"
